    <queries>
        <package android:name="com.tencent.mm" />            <!-- 指定微信包名-->
        <package android:name="com.tencent.mobileqq" />   <!-- 指定qq包名-->
        <package android:name="com.sina.weibo" />           <!-- 指定微博包名-->
        <package android:name="com.tencent.wework" />      <!-- 指定企业微信包名-->
