<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.cloudwalk.libproject"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="1" />

    <!-- <uses-sdk -->
    <!-- android:minSdkVersion="14" -->
    <!-- android:targetSdkVersion="19" /> -->

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />
    <uses-feature android:name="android.hardware.camera.flash" />

    <uses-permission android:name="android.permission.WRITE_SETTINGS" />

    <application>
        <activity
            android:name="cn.cloudwalk.libproject.OcrCameraActivity"
            android:label="@string/app_name"
            android:screenOrientation="landscape" />
        <activity
            android:name="cn.cloudwalk.libproject.LiveResultActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="cn.cloudwalk.libproject.LiveActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="cn.cloudwalk.libproject.LiveStartActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="cn.cloudwalk.libproject.CloudwalkBankCardOCRActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/app_name"
            android:screenOrientation="landscape" />
        <activity
            android:name="cn.cloudwalk.libproject.BankCardResultActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
    </application>

</manifest>